:host([device="large"]) .mentor .mentor__content-inner{
    min-height: 15.625em;
}

.mentor-inner-container{
    position: absolute;
    bottom: 0px;
    display: flex;
}

.mentor .mentor__close{
    position: absolute;
    top: 1px;
    right: 1px;
    border: none;
    padding: 0;
    box-sizing: border-box;
    background: rgb(var(--background-color-2));
    color: var(--font-color-2);
    display: inline-block;
    font-weight: normal;
    height: 3.75rem;
    text-align: center;
    width: 3.75rem;
    transition: all .3s ease-in-out;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor:pointer;
}

.mentor .mentor__content .mentor__close .material-close-icon {
    transform: scale(1.2);
}

:host([theme="dark"]) .mentor .mentor__content .mentor__close .material-close-icon{
