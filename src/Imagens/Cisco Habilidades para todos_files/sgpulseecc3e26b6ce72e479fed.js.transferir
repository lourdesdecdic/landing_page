        width: inherit;
        max-width: 50rem;
    }

    .mentor__image{
        margin-left: auto;
        margin-right: auto;
        bottom: 3.75rem;
        left: 0;
        min-width: 51.5%;
        position: absolute;
        width: 51.5%;
    }

    .mentor__content {
        position: absolute;
        right: 0;
        bottom: 25%;
        width: 51.5%;
    }

    .mentor .mentor__content-inner:before{
        left: -0.88rem;
        bottom: 10%;
    }
}

@media only screen and (min-width: 32.5rem) and (max-width: 47.4375rem){
    .mentor .mentor__image {
        width: 80%;
    }
}

@media only screen and (max-width: 47.5rem){
    .mentor__inner {
        display: flex;
        align-items: center;
    }

    .mentor__image {
        min-width: auto;
        width: 70%;
    }

    .mentor-inner-container {
        max-width: 34.375rem;
        display: flex;
        flex-direction: column-reverse;
        margin-left: auto;
        margin-right: auto;
    }

    .mentor .mentor__content-inner:before{
        border-color: rgb(var(--background-color-3)) transparent transparent transparent;
        border-width: 0.9375rem 0.9375rem 0 0.9375rem;
        top: 99%;
        transform: translateX(-50%);
    }

}

@media only screen and (min-width: 64rem){
    .mentor-inner-container {
        max-width: 64rem;
    }
}

