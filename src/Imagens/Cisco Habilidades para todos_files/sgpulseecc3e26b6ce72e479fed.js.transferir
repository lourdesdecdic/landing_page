.mentor {
    height: 100%;
    position: fixed;
    top: 0;
    width: 100%;
    z-index:500;
}

.mentor:after{
    content: '';
    height:100%;
    position:fixed;
    bottom:0;
    width:100%;
    left:0;
    background-color: rgba(26,32,53,0.8);
    z-index: -1;
    display: block;
}
:host([theme="dark"]) .mentor:after{
    background-color: rgba(42,42,42,0.8);
}

.mentor .mentor__content-inner{
    box-sizing: border-box;
    background-color: rgb(var(--background-color-3));
    color: var(--font-color-1);
    height: auto;
    margin-left: auto;
    margin-right: auto;
    text-align: left;
    width: 100%;
    display: flex;
    flex: 1;
    flex-direction: column;
    padding: 6%;
    border: .0625rem solid rgb(var(--background-color-1));
