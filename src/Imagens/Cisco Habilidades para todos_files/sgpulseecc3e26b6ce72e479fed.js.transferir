        .adaptive-toolbar-container .abs__skip-button .skip-inner.is-skip-selected {
            background-color: #FFEDD8;
            border-bottom: 4px solid #F5AC76;
        }

        .adaptive-toolbar-container .abs__skip-button .skip-inner input[type=checkbox] {
            transform: scale(1.4);
            padding: 10px;
            accent-color: #6abf4b;
