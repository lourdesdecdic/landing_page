    .ways-to-use-cpt-header-section-component-graphic .graphic__image-container  {
      position: relative;
      height: 100%;
    }

    .ways-to-use-cpt-header-section-component-graphic .graphic__image-container{
      padding-top: 0 !important;
    }

    .ways-to-use-cpt-body{
      margin-bottom: 16px;
    }

    .article.ways-to-use-cpt-header-section-article{
      background-color: #F8F8F8;
      display: flex;
      justify-content: center;
    }

    .article.ways-to-use-cpt-header-section-article .no-adaptive-scroll{
      display: flex;
    }

    .article.ways-to-use-cpt-header-section-article .no-adaptive-scroll div:has(.block__container){
      display: flex;
    }

    .article.ways-to-use-cpt-header-section-article .no-adaptive-scroll .block__container{
      display: flex;
    }

    .ways-to-use-cpt-header-section-block{
      display: flex;
    }

    .ways-to-use-cpt-header-section-block .block__inner{
      display: flex;
      margin: 0 !important;
    }

    :host(.is-left.ways-to-use-cpt-header-section-component){
      width: 100%;
      padding-left: 40px;
      padding-right: 40px !important;
    }

    :host(.is-right.ways-to-use-cpt-header-section-component-graphic){
      width: 100%;
    }

    @media only screen and (max-width: 770px){
      :host(.is-right.ways-to-use-cpt-header-section-component-graphic){
        padding-left: 0;
      }
    }

    @media only screen and (min-width: 770px){
      :host(.is-left.ways-to-use-cpt-header-section-component){
        width: 60%;
          display: flex;
          align-items: center;
          padding-left: 40px;
      }

      :host(.is-right.ways-to-use-cpt-header-section-component-graphic){
      width: 40%;
      }

      .ways-to-use-cpt-header-section-component-graphic .graphic__image{
          height: auto;
          object-fit: cover;
          min-height: 100%;
      }
    }

    @media only screen and (max-width: 770px){
      :host(.is-left.ways-to-use-cpt-header-section-component){
        width: 100%;
      }
