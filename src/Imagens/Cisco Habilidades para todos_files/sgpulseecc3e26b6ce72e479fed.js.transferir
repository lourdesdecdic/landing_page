/*# _dark_theme.less dark theme css*/

.ace-twilight .ace_keyword, .ace-twilight .ace_meta {
  color: #66d9ef !important;
}

.ace-twilight .ace_string {
  color: #a6e22e !important;
}

.ace-twilight .ace_constant,
.ace-twilight .ace_constant.ace_character,
.ace-twilight .ace_constant.ace_character.ace_escape,
.ace-twilight .ace_constant.ace_other,
.ace-twilight .ace_heading,
.ace-twilight .ace_markup.ace_heading,
.ace-twilight .ace_support.ace_constant {
  color: #ae81ff !important;
}

.ace-twilight .ace_operator {
  color: #f8f8f8 !important;
}

.ace-twilight .ace_comment {
  color: #8292a2 !important;
}

.ace-twilight, .ace-twilight .ace_gutter {
  background-color: #272822 !important;
}

.ace-twilight .ace_gutter {
  color: #999 !important;
}

:host([theme="dark"]) .edubeeditor__box {
  box-shadow: 0 1px 2px rgba(0,0,0,0.4), 0 2px 8px rgba(0,0,0,0.4);
}

.edubeeditor-icons-mode .material-icons {
  font-size: 14px !important;
}

.edubeeditor__editor .ace_comment{
  font-style: normal;
}

.edubeeditor-popup__close material-icon{
  color: var(--black-color);
}

