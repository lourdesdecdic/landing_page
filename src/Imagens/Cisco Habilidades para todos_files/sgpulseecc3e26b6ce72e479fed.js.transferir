.codeWindowWithMcq-container {
    margin-bottom: 3%;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper {
    position: relative;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper .button-copy {
    display: flex;
    flex-direction: row;
    align-items: center;
    gap: 10px;
    position: absolute;
    height: 30px;
    top: 5px;
    right: 20px;
    z-index: 5;
    background-color: transparent;
    color: white;
    border-radius: 4px;
    font-size: 14px;
    border:0;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper .button-copy svg {
    width: 16px;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre {
    padding: 1em 0 1em 3em;
    padding-left: 4.65em;
    overflow-x: auto;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre code {
    position: relative;
    white-space: initial;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre.python,
.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre.javascript,
.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre.c {
    position: relative;
    color: #b7bec9;
    background: #262729;
    white-space: initial;
    border-radius: 0.3em;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre.python:not(.line-numbers),
.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre.javascript:not(.line-numbers),
.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre.c:not(.line-numbers)
 {
    padding-left: 1em !important;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre.line-numbers .code-number {
    position: absolute;
    top: 0.8em;
    left: 0em;
    width: 1.8em;
    pointer-events: none;
    user-select: none;
    text-align: right;
    padding-right: 1rem;
    border-right: 1px solid #999;
    color: #999;
    white-space: initial;
    font-size: 1rem;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre .code-wrapper {
    padding-left: 1rem;
    white-space: initial;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre .command {
    position: relative;
    font-family: Consolas, Monaco, 'Andale Mono', 'Ubuntu Mono', monospace;
    font-size: 1em;
}
.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre {
    font-size: 1em;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre .command.hljs {
    background-color: transparent;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre .command.highlight-line {
    background: linear-gradient(to right, hsla(24, 20%, 50%, 0.1) 70%, hsla(24, 20%, 50%, 0));
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre .hljs-built_in {
    color: #66d9ef !important;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre .hljs-number,
.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre .hljs-literal {
    color: #ae81ff !important;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre .hljs-string {
    color: #a6e22e !important;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre.normal-syntax {
    background-color: #e1e1e1;
    color: #333;
}
.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre.normal-syntax {
    background-color: #e1e1e1 !important;
    color: #333 !important;
}

.codeWindowWithMcq-container .code_window_with_mcq_wrapper pre.output {
    background-color: #e1e1e1;
    padding-top: 2.2rem;
}

.hljs {
    border-radius: 0.5rem;
}

.hljs .color-orange {
    color: #fbab18;
}

.hljs .color-green {
    color: #6ebe4a;
}

.hljs .color-purple {
    color: #b45bcf;
}

.codeWindowWithMcq-container.syntax-wrong .codeWindow__inner pre .hljs {
    color: red;
}

.codeWindowWithMcq-container.syntax-wrong .codeWindow__inner pre div,
.codeWindowWithMcq-container.syntax-wrong .codeWindow__inner pre span {
    color: red !important;
}

.codeWindowWithMcq-container.syntax-wrong .codeWindow__inner pre .code-number div:first-child,
.codeWindowWithMcq-container.syntax-wrong .codeWindow__inner pre .code-number div:last-child {
    color: red !important;
}

.label-output {
    position: absolute;
    top: 0;
    right: 0;
    padding: 0.2em 0.6em 0.3em;
    font-weight: 700;
    color: #fff !important;
    text-align: center;
    white-space: nowrap;
    vertical-align: baseline;
    background-color: #9096A0;
    font-size: 1rem;
}
codep {
    display: block;
    width: 100%;
    overflow-x: auto;
    font-family: monospace;
    white-space: pre-wrap;
    background: #9096a0;
    color: white;
    padding: .125rem .3125rem .0625rem;
    border-radius: .3em;
    font-weight: bold;
    word-break: break-word;
}

.codeWindowWithMcq-container .getOutputMainClass .getOutputClass {
    font-family: monospace !important;
    font-size: 1rem !important;
  }

.getOutputMainClass {
    position: relative;
    background-color: rgb(var(--background-color-2));
    padding-top: calc(var(--padding-top) / 4);
    padding-left: calc(var(--padding-left) / 4);
    padding-bottom:  calc(var(--padding-bottom) / 4);
    border-radius: var(--border-radius);
    overflow: hidden;
    margin-bottom: calc(var(--margin-bottom) / 3);

}
.getOutputMainClass .getOutputTextClass {
    width: 85%;
    font-family: Consolas, Monaco, 'Andale Mono', 'Ubuntu Mono', monospace;
}
.getOutputMainClass .getOutputClass {
    position: absolute;
    top: 0;
    right: 0;
    background-color: rgb(144, 150, 160);
    color: rgb(var(--white-color));
    font-weight: 700;
    padding: 0.2em .6em 0.3em;
}

