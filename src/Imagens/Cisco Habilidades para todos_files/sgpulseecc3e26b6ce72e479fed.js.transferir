            z-index: 89;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            border-top: 2px solid rgb(var(--background-color-2));
        }

        .adaptive-toolbar-container .abs__btn-arrow-container .btn-icon {
            border: 2px solid rgb(var(--primary-color));
            padding: 10px;
            border-radius: 30px;
            font-size: .8em;
            text-decoration: none;
            width: 140px;
            display: flex;
            justify-content: center;
        }

        .adaptive-toolbar-container .abs__btn-arrow-container .btn-icon.is-disabled {
            background-color: rgb(var(--disable-button-color));
            color: rgb(var(--font-color-2));
            border: var(--border-width) solid rgb(var(--disable-button-color));
            opacity: 0.5;
        }



