    width: calc(100% - 2.75rem);
    height: 100%;
}

.mentor__image{
    margin-left: auto;
    margin-right: auto;
    z-index: 0;
}

.mentor__inner{
    height: 100%
}

.mentor__content {
    position: relative;
}

