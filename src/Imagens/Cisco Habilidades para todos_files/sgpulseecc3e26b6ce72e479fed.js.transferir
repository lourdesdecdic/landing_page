    .ways-to-use-cpt-header-section-content h2{
      font-size: 40px;
      font-weight: 100;
      margin-bottom: 20px;
      margin-top: 20px;
    }

    .ways-to-use-cpt-header-section-content{
      margin-bottom: 40px;
    }

    .ways-to-use-cpt-header-section-content p{
      font-size: 16px;
      font-weight: 300;
    }

    .ways-to-use-cpt-header-section-details{
