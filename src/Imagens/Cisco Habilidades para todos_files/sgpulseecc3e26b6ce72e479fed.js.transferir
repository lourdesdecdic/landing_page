.likert-question{
    display:block;
    font-size: var(--font-size-base);
    font-weight: bold;
    background-color: rgb(var(--background-color-5));
    color: rgb(var(--font-color-1));
    font-size: var(--font-size-base);
    padding: calc(var(--padding-top) / 3.2) calc(var(--padding-right) / 1) calc(var(--padding-bottom) / 3.2) calc(var(--padding-left) / 1);
    position: relative;
}


.likert-question-item-container{
    background-color: rgb(var(--item-color));
    color: rgb(var(--item-color-inverted));
    position: relative;
    border: var(--border-width) solid rgb(var(--item-border-color));
    border-radius: var(--border-radius);
    margin-bottom: calc(var(--margin-bottom) / 6.6);
}

.likert-item-wrapper{
    padding: calc(var(--padding-top) / 4);
    overflow-x: auto;
}
:host([device="small"]) .likert-question-item-container .likert-item-wrapper{
    overflow-x: inherit !important;
}
.likert-item-container{
   display: flex; 
   justify-content:space-around;
   position: relative;
   width: 100%;
}

.likert-item-container{
   min-width: max-content;
}
   
.likert-item{
    display: flex;
    align-items: center;
    justify-content: space-between;
    flex-direction: column;
    z-index: 2;
    text-align: center;
    width: 100%;
}
.likert-item .component-item-text-color{
   background-color: transparent;
}
.likert-item material-icon{
    z-index: 2;
}
.likert-item-container:after {
    content:"";
    position: absolute;
    width:100%;
    height:3em;
    background-color: rgba(var(--background-color-2));
    bottom:0;
    z-index: 1;
    border-radius: var(--border-radius);
}
.likert-item-text{
    padding:calc(var(--padding)/8);
    margin-bottom: calc(var(--margin-bottom)/4);
}
.likert .likert__inner .component__widget.is-disabled{
    pointer-events:none;
}
.likert .likert__inner .component__widget{
    margin-bottom: calc(var(--margin-bottom)/1.3);
}
.likert-radio-checked,.likert-radio-unchecked{
    cursor: pointer;
    transform: scale(1.2);
    color: rgba(var(--item-border-color));
    background-color: rgba(var(--background-color-0));
    height: 1.2em;
    width: 1.2em;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 50%;
    margin-bottom: calc(var(--margin-bottom)/4.5);
}
.likert-radio-checked{
    color:rgba(var(--item-color-selected));
    background: rgba(var(--white-color));
}

.likert_item-state{
    left: 0.8em;
    height: 100%;
    line-height: calc(var(--line-height-regular) * 2.5);
    width: 3em;
    margin-top: 0;
    position: absolute;
    text-align: center;
    z-index: 3;
    color: rgb(var(--black-color));
    top: 0;
    transform: translateY(0%);
    display: flex;
    align-items: center;
    justify-content: center;
}

@media only screen and (max-width: 519px){
    .likert_item-state{
        width: initial;
    }

    .likert-item-container.is-correct .likert_item-state{
        width: fit-content;
        margin-right: calc((var(--margin-right)) - 3.5em);
        display: flex;
        align-items: center;
    }

    .likert__select-container, .likert-dropdown__btn{
        width: 100%;
    }
    .likert-dropdown__list{
        position: absolute;
        top: 100%;
        background: rgb(var(--item-color));
        z-index: 1;
        list-style-type: none;
        width: 100%;
        border: var(--border-width) solid rgb(var(--item-border-color));
        margin-top: calc((var(--margin-top)) - 4em);
        padding: 0;
    }
    .likert-dropdown__item{
        padding: calc((var(--padding)) - 3.7em) calc((var(--padding)) - 3.4em);
        cursor: pointer;
    }
    .likert-dropdown__item:hover{
        background-color: rgb(var(--item-color-hover));
        color: rgb(var(--item-color-inverted-hover));
    }
    .likert-dropdown__btn{
        display: flex;
        justify-content: space-between;
        align-items: center;
        padding: calc((var(--padding)) - 3.4em);
        background: rgba(var(--background-color-2));
        color: rgb(var(--item-color-inverted));
    }
    .likert-dropdown__btn-select-text{
        margin-right: auto;
    }
    .likert-question-item-container:has(.selected) .likert-dropdown__btn-select-text{
        display: none;
    }

    .likert-dropdown__btn material-icon{
        color: rgb(var(--item-color-inverted));
    }

    .likert-item-container:after{
        display: none;
    }
